
public class Animal {

	public Animal() {
		System.out.println("in animal constructor");
	}

	int breathe() {
		System.out.println("Animal breathes...");
		return 0;
	}
	
	int mobility() {
		System.out.println("Animal moves....");
		return 0;
	}
	
	int makeSound() {
		System.out.println("Animals make sound....");
		return 0;
	}
}

