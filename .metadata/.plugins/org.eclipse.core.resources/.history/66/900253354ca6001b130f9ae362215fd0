
public class FordDemo {

	public static void main(String[] args) {
		FordFigo ff1 = new FordFigo("FF101", 1234);
		ff1.switchOn();
		ff1.breakingSystem();
		ff1.switchOff();
		
		System.out.println("------------------------------");
		
		FordFigoTitanium fft1 = new FordFigoTitanium("FFT201", 7891);
		fft1.switchOn();
		fft1.breakingSystem();
		fft1.airbagsWorking();
		fft1.switchOff();
	}

}
