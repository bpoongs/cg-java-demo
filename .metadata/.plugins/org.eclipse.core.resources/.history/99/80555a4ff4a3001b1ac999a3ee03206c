
public class Room {
	
	int length;
	int breadth;
	int height;
	
	int calculateFloorArea() {
		int floorArea = 0;
		floorArea = length * breadth;
		return floorArea;
	}
	
	int calculatePaintingCost() {
		int paintingCost = 0;
		paintingCost = 2 * (2 * height * (length + breadth));
		return paintingCost;
	}
}
