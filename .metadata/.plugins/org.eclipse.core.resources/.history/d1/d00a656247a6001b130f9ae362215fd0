
public class FordFigo {
	String carModelNo;
	int chasisNo;
	String color;
	
	int switchOn() {
		System.out.println("Ford Figo switched on.....");
		return 0;
	}
	
	int switchOff() {
		System.out.println("Ford Figo switched off.....");
		return 0;
	}
	
	int breakingSystem() {
		System.out.println("Ford figo manual breaking system.....");
		return 0;
	}
}
